{% extends 'partials/base.html.twig' %}

{% block content %}
    {% for p in page.collection %}
        <div class="content">
            <h3 class="title is-4"><a href="{{ p.permalink }}">{{ p.title }}</a></h3>
            <p class="is-italic">{{ p.header.description }}</p>
        </div>
        <hr>
    {% endfor %}
{% endblock %}


